@model Szakdoli.Models.Keszlet

@{
    ViewData["Title"] = "Részletek";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Részletek</h1>

<div>
    <h4>Készlet</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TermekTipus)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TermekTipus.TipusNev)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Raktar)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Raktar.Nev)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Mennyiseg)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Mennyiseg)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Vissza a lista nézethez</a>
</div>
